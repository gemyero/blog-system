    "mysql2": "^2.2.5",
    "sequelize": "^6.3.5",
    "swagger-jsdoc": "^6.0.1",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "axios": "^0.21.1",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
